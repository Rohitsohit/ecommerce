{
    "version" : 2,
    "builds":[
        {
            "src":"index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [{

        "src":"/(.*)",
        "dest" : "/app.js",
        "methods" : ["GET","POST","DELETE","PUT","PATCH","OPTIONS"],
        "Access-Control-Allow-Origin" : "*"

    }]
}